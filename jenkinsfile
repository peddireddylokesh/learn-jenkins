    agent any
    stages {
        stage('Build') {
            steps {
                script{
                    sh "thi is Building..."
                }
            }
        }
        stage('Test') {
            steps {
                  script{
                    sh "thi is testing..."
                }
            }
        }
        stage('Deploy') {
            steps {
                  script{
                    sh "thi is deploying..."
                }
            }
        }
    }
}
pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                
                sh "echo 'This is Building...'"
                
            }
        }
        stage('Test') {
            steps {
                  
                sh "echo 'This is Testing...'"
                
            }
        }
        stage('Deploy') {
            steps {
                  
                sh "echo 'This is Deploying...'"
            }
            
        }
    }
}